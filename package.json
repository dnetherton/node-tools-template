{
  "name": "node-tools-template",
  "version": "0.0.1",
  "description": "A template to create a set of isolated node scripts",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix scripts",
    "lint:nofix": "eslint scripts",
    "build": "./build.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "12.6.0"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      [
        "@babel/plugin-proposal-do-expressions"
      ],
      [
        "@babel/plugin-proposal-nullish-coalescing-operator"
      ],
      [
        "@babel/plugin-proposal-optional-chaining"
      ],
      [
        "@babel/plugin-proposal-partial-application"
      ],
      [
        "@babel/plugin-proposal-throw-expressions"
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "useBuiltIns": "usage",
          "targets": {
            "node": "12.6.0"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-partial-application": "^7.4.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.3.0",
    "blessed": "^0.1.81",
    "capitalize": "^2.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chalk": "^2.4.2",
    "columnify": "^1.5.4",
    "commander": "^2.20.0",
    "core-js": "^3.1.4",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-stickler": "^1.0.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "eslint-plugin-node": "^9.1.0",
    "humanize-string": "^2.1.0",
    "mocha": "^6.1.4",
    "ora": "^3.4.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "strip-ansi": "^5.2.0",
    "table-layout": "^1.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "aws-sdk": "^2.504.0"
  }
}
